  if    test -f $srcdir/config/$x
  then      bx="$bx $x"
  elif  echo $x | grep '\(a-z0-9a-z0-9*\)/xm-\1\.h' >/dev/null
  then      :
  else      echo "configure: warning: $srcdir/config/$x does not exist." 1>&2
  fi
done
